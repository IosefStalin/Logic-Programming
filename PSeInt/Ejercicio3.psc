Algoritmo Ejercicio3
	definir nota1, nota2, nota3 Como Real;
	nota1 <- 0;
	nota2 <- 0;
	nota3 <- 0;
	Escribir "Escribe las 3 notas:"
	Leer nota1;
	Leer nota2;
	Leer nota3;
	notaTotal <- (nota1 + nota2 + nota3)/3;
	Imprimir "La media ponderada de todas las notas es " notaTotal;
FinAlgoritmo